                set only actual step:<br />
                            to value:
              </td>
              <td>
                <input type="checkbox" result="Hauppauge[1]__choice:set_steps" />
                <input type="spinner" result="Hauppauge[1]__choice:digits" width="20" min="1" max="5" />
                &#160;digits<br />
                <input type="spinner" result="Hauppauge[1]__choice:to_value" width="50" min="0" max="99999" />
              </td>
            </tr>
            <tr>
              <td align="right">
                wait after:
              </td>
              <td>
                <input type="text" value="sec" format="#1.3" result="Hauppauge[1]__choice:after" /><br />
                <input type="text" value="items" result="Hauppauge[1]__choice:calculate_lirc" />
              </td>
            </tr>
