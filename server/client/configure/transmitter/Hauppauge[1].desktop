          <table>
            <tr>
              <td align="right">
                button:
              </td>
              <td>
                <input type="spinner" result="Hauppauge[1]__choice:group" width="20" min="1" max="47" />&#160;
                <input type="text" result="Hauppauge[1]__choice:correct_group" value="" disabled="disabled" width="20"/>
              </td>
            </tr>
            <tr>
              <td align="right">
                steps:
              </td>
