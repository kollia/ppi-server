          <table>
            <tr>
              <td align="right">
                button:
              </td>
              <td>
                <input type="spinner" result="Hauppauge_350__choice:group" width="20" min="1" max="44" />&#160;
                <input type="text" result="Hauppauge_350__choice:correct_group" value="" disabled="disabled" width="20"/>
              </td>
            </tr>
            <tr>
              <td align="right">
                steps:
              </td>
              <td>
                <input type="spinner" result="Hauppauge_350__choice:steps" />
              </td>
            </tr>
            <tr>
              <td>
              </td>
              <td>
                <select size="1" result="Hauppauge_350__choice:steps_action">
                  <option value="0">UP STOP</option>
                  <option value="1">DOWN STOP</option>
                  <option value="2">UP LOOP</option>
                  <option value="3">DOWN LOOP</option>
                </select>
              </td>
            </tr>
            <tr>
              <td>
              </td>
              <td>
                <select size="1" result="Hauppauge_350__choice:transmit_action">
                  <option value="0">SEND ONCE</option>
                  <option value="1">SEND</option>
                  <option value="2">WAIT after</option>
                  <option value="3">BACK time</option>
                </select>
              </td>
            </tr>
            <tr>
              <td align="right">
                <input type="checkbox" result="Hauppauge_350__choice:receive" readonly="readonly" />
              </td>
              <td>
                <select size="1" result="Hauppauge_350__choice:correct_group" disabled="disabled">
                  <option value="1">Go</option>
                  <option value="2">Power</option>
                  <option value="3">TV</option>
                  <option value="4">Videos</option>
                  <option value="5">Music</option>
                  <option value="6">Pictures</option>
                  <option value="7">Guide</option>
                  <option value="8">Radio</option>
                  <option value="9">Up</option>
                  <option value="10">Left</option>
                  <option value="11">Right</option>
                  <option value="12">Down</option>
                  <option value="13">OK</option>
                  <option value="14">Back/Exit</option>
                  <option value="15">Menu/i</option>
                  <option value="16">Vol+</option>
                  <option value="17">Vol-</option>
                  <option value="18">Prev.Ch</option>
                  <option value="19">Mute</option>
                  <option value="20">Ch+</option>
                  <option value="21">Ch-</option>
                  <option value="22">Record</option>
                  <option value="23">Stop</option>
                  <option value="24">Rewind</option>
                  <option value="25">Play</option>
                  <option value="26">Forward</option>
                  <option value="27">Replay/SkipBackward</option>
                  <option value="28">Pause</option>
                  <option value="29">SkipForward</option>
                  <option value="30">1</option>
                  <option value="31">2</option>
                  <option value="32">3</option>
                  <option value="33">4</option>
                  <option value="34">5</option>
                  <option value="35">6</option>
                  <option value="36">7</option>
                  <option value="37">8</option>
                  <option value="38">9</option>
                  <option value="39">Asterix</option>
                  <option value="40">0</option>
                  <option value="41">Red</option>
                  <option value="42">Green</option>
                  <option value="43">Yellow</option>
                  <option value="44">Blue</option>
                </select>
                <br />
                <input type="button" value="PRESS" result="Hauppauge_350__choice:count" />
              </td>
            </tr>
            <tr>
              <td align="right">
                first touch show:
              </td>
              <td>
                <input type="checkbox" result="Hauppauge_350__choice:display_first" />
              </td>
            </tr>
            <tr>
              <td align="left">
                wait double<br />
                after pressed again:
              </td>
              <td>
                <input type="checkbox" result="Hauppauge_350__choice:double" />
              </td>
            </tr>
            <tr>
              <td align="right">
                set only actual step:<br />
                            to value:
              </td>
              <td>
                <input type="checkbox" result="Hauppauge_350__choice:set_steps" />
                <input type="spinner" result="Hauppauge_350__choice:digits" width="20" min="1" max="5" />
                &#160;digits<br />
                <input type="spinner" result="Hauppauge_350__choice:to_value" width="50" min="0" max="99999" />
              </td>
            </tr>
            <tr>
              <td align="right">
                wait after:
              </td>
              <td>
                <input type="text" value="sec" format="#1.3" result="Hauppauge_350__choice:after" /><br />
                <input type="text" value="items" result="Hauppauge_350__choice:calculate_lirc" />
              </td>
            </tr>
            <tr>
              <td>
              </td>
              <td>
